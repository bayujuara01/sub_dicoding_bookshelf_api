{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "comma-dangle": "off",
        "no-unused-vars":"off",
        "class-methods-use-this":"off",
        "object-curly-newline":"off",
        "eqeqeq":"off"
    }
}
